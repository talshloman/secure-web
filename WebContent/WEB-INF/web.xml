		<url-pattern>/CheckItem</url-pattern>
	</servlet-mapping>
			<servlet-mapping>
		<servlet-name>CreditCardController</servlet-name>
		<url-pattern>/CreditCard</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>Pictures</servlet-name>
	<url-pattern>/Pictures/*</url-pattern>
</servlet-mapping>
			<servlet-mapping>
		<servlet-name>SearchItemController</servlet-name>
		<url-pattern>/SearchItem</url-pattern>
	</servlet-mapping>


</web-app>